      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="StudentInfoes">
      <EntityTypeMapping TypeName="SmartEducationModel.StudentInfo">
        <MappingFragment StoreEntitySet="StudentInfo">
