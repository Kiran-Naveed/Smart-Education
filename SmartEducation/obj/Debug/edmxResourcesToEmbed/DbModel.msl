        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Courses">
      <EntityTypeMapping TypeName="SmartEducationModel.Cours">
        <MappingFragment StoreEntitySet="Courses">
          <ScalarProperty Name="cId" ColumnName="cId" />
