    <FunctionImportMapping FunctionImportName="timeTable" FunctionName="SmartEducationModel.Store.timeTable">
      <ResultMapping>
        <ComplexTypeMapping TypeName="SmartEducationModel.timeTable_Result">
          <ScalarProperty Name="CourseName" ColumnName="CourseName" />
          <ScalarProperty Name="Day" ColumnName="Day" />
          <ScalarProperty Name="StartTime" ColumnName="StartTime" />
