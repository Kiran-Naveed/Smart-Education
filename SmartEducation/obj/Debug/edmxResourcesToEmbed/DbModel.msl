<?xml version="1.0" encoding="utf-8"?>
<Mapping Space="C-S" xmlns="http://schemas.microsoft.com/ado/2009/11/mapping/cs">
  <EntityContainerMapping StorageEntityContainer="SmartEducationModelStoreContainer" CdmEntityContainer="SmartEducationEntities">
    <EntitySetMapping Name="Attendances">
      <EntityTypeMapping TypeName="SmartEducationModel.Attendance">
        <MappingFragment StoreEntitySet="Attendance">
          <ScalarProperty Name="LecturesId" ColumnName="LecturesId" />
          <ScalarProperty Name="eId" ColumnName="eId" />
          <ScalarProperty Name="InfoType" ColumnName="InfoType" />
          <ScalarProperty Name="Value" ColumnName="Value" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="ClassSchedules">
      <EntityTypeMapping TypeName="SmartEducationModel.ClassSchedule">
