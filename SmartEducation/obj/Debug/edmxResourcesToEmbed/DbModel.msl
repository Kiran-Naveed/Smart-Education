        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="LectureScheduals">
      <EntityTypeMapping TypeName="SmartEducationModel.LectureSchedual">
        <MappingFragment StoreEntitySet="LectureSchedual">
          <ScalarProperty Name="LsId" ColumnName="LsId" />
